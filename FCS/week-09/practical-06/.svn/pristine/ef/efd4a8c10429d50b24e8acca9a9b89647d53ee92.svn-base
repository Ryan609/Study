public class Node {

	private String data;
	private Node next;

	//Basic constructor
	public Node() {
		this.next = null;
	}

	//Implement the constructor
	public Node(String data) {
		this.data = data;
		this.next = null;
	}

	//get and set Method
	public String getData() {
		return data;
	}

	public void setData(String data) {
		this.data = data;
	}

	public Node getNext() {
		return next;
	}

	public void setNext(Node next) {
		this.next = next;
	}

	//Create printnode method
	public void printNode() {
		System.out.printf(" "+ data + " ");
	}

}