import java.util.Scanner;

class input {
    public static void main(String[] args) {
    	
    	Scanner input = new Scanner(System.in);
    	
        System.out.print("What's your name: ");
        String myString = input.nextLine();
        

    	// Getting float input
    	System.out.print("What's your weight(Kg): ");
    	float myFloat = input.nextFloat();
    	
    	
    	// Getting double input
    	System.out.print("What's your high(CM): ");
    	double myDouble = input.nextDouble();
    	
    	
    	// Getting String input
        input.nextLine();

    	System.out.print("What is your favorite thing: ");
    	String myString1 = input.nextLine();

        System.out.println("Your name is " + myString);
        System.out.println("Your weight is " + myFloat + "Kg");
        System.out.println("Your high is " + myDouble + "CM");
    	System.out.println("Your favorite thing is " + myString1);
    }
}