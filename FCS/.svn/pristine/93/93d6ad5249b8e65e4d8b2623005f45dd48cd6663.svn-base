public class Problem {

   public static void main(String args[]) {
        //Use loop to get 40 numbers.
       	for(int i = 1; i < 41; i++) {

            //Use nested loops to obtain symbols equal to the number of numbers
       		for (int j = 0; j < i; j++){
       			//Prescribe each symbol
			    if ( i < 10) {
				    System.out.print( "*");
			    } else if (i < 20) {
				    System.out.print( "=");
			    } else if (i < 30) {
				    System.out.print( "x");
			    } else 		
			       System.out.print( "o");
			}
			       System.out.println();
		}
   }
}