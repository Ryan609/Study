public class LinearSearch extends MysearchAlg {

	//Override the search method
	@Override
	public int search(int[] array, int num) {
		//Use a for loop to traverse the array
		for (int i = 0; i < array.length; i++) {
			/*
			Use the if statement to determine 
			whether the array element is equal to the input number
			*/
			if (array[i] == num) {
				return i;
			}
		}
		//If not found, return -1
		return -1;
	}
}